<%- include("partials/header.ejs"); -%>
<h1>Compose</h1>
<form class="" action="/" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" class="form-control" id="title" name="newTitle" placeholder="....">
  </div>
  <div class="form-group">
    <label for="post">Post</label><br />
    <textarea id="post" name="newPost" rows="8" cols="100%"></textarea>
  </div>
  <input class="btn btn-primary" type="submit" name="newItem" value="Publish">
</form>
<%- include("partials/footer.ejs"); -%>
